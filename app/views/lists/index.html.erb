<h1>All Lists</h1>

<% @lists.each do |list| %>

<ul>
  <li><%= list.name %><%= link_to ' Edit', edit_list_path(list) %><%= link_to ' Delete', list, 
  																			confirm: 'Are you sure?',
  																			method: :delete %>
  																  <%= link_to 'Show', list_path(list) %></li>
</ul>

<% end %>  


<p><%= link_to "New List", new_list_path %></p>